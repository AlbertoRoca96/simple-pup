  "react-dom": "^18.3.1"
 },
 "devDependencies": {
  "@axe-core/playwright": "^4.9.1",
  "@playwright/test": "^1.47.2",
  "@types/node": "^24.9.1",
  "@types/react": "^18.3.8",
  "@types/react-dom": "^18.3.0",
  "@vitejs/plugin-react": "^4.3.2",
  "autoprefixer": "^10.4.20",
  "postcss": "^8.4.47",
  "tailwindcss": "^3.4.14",
  "typescript": "^5.6.3",
  "vite": "^5.4.10",
  "vitest": "^2.1.3"
 },
 "engines": {
  "node": ">=20"
 }
