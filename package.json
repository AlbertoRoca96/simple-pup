{
 "name": "simple-pup",
 "version": "1.0.0",
 "private": true,
 "type": "module",
 "description": "Walmart product search (React + Vite) with Playwright E2E and GitHub Pages deploy.",
 "scripts": {
  "dev": "vite",
  "build": "tsc -b && vite build",
